	<Target Name="ZipNexusRelease" AfterTargets="Build" Condition="'$(Configuration)' == 'NEXUSRELEASE'">
		<ItemGroup>
			<FilesToZip Include="$(TargetDir)$(TargetFileName)" />
			<FilesToZip Include="manifest.json" Condition="Exists('manifest.json')" />
			<FilesToZip Include="icon.png" Condition="Exists('icon.png')" />
		</ItemGroup>

		<PropertyGroup>
			<DownloadDir>F:\Downloads\</DownloadDir>
			<ZipOutputPath>$(DownloadDir)$(AssemblyName)_v$(Version).zip</ZipOutputPath>
		</PropertyGroup>

		<MakeDir Directories="$(DownloadDir)" />

		<PropertyGroup>
			<FilesList>$([System.String]::Join(',', %(FilesToZip.FullPath)))</FilesList>
		</PropertyGroup>

		<Exec Command="powershell -NoLogo -NoProfile -Command &quot;Compress-Archive -Path $(FilesList) -DestinationPath '$(ZipOutputPath)' -Force&quot;" />
	</Target>





	<Target Name="PostBuild" AfterTargets="PostBuildEvent" Condition="'$(Configuration)' == 'RELEASE'">
		<Exec Command="COPY &quot;$(TargetPath)&quot; &quot;E:\SteamLibrary\steamapps\common\Last Epoch\Mods&quot;" />
	</Target>

